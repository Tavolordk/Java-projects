package figura;

public class ReturnObject {
	public static void main(String [] args)
	{
		Suma s = new Suma (4,3);
	}
	private static Suma creaObjetoSuma()
	{
		return new Suma(4,6);
	}
}

class Suma
{
	int a, b;
	public Suma(int a, int b)
	{
		this.a = a;
		this.b = b;
	}
	public int sumar()
	{
		return this.a + this.b;
	}
}