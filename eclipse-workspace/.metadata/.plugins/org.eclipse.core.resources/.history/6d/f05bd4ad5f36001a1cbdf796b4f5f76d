package figura;

public class Caja {
	int ancho, alto, profundo;
	public Caja()
	{
		System.out.println("Este es el constructor vacío");
		this.ancho=3;
		this.alto=2;
		this.profundo=6;
	}
	
	public Caja(int a, int b, int c)
	{
		this.ancho=a;
		this.alto=b;
		this.profundo=c;
	}

}
