package figura;

public class Aritmetica {
	int a,b;
	
	public Aritmetica()
	{
		a=5;
		b=4;
		System.out.println("Ejecutando constructor vacío :D ");
	}
	
	public Aritmetica(int a, int b)
	{
		System.out.println("Ejecutando constructor 2 ");
		this.a=a;
		this.b=b;
	}

	public int sumar()
	{
		return a+b;
	}
	
	public int restar()
	{
		return a-b;
	}
	
	public int multiplicar()
	{
		return a*b;
	}
	
	public int dividir()
	{
		return a/b;
	}
}
