package Web;
import java.io.Serializable;
public class Rectangulo implements Serializable{
    private int base;
    private int altura;
	public int getBase() {
		return this.base;
	}
	public void setBase(int base) {
		this.base = base;
	}
	public int getAltura() {
		return this.altura;
	}
	public void setAltura(int altura) {
		this.altura = altura;
	}

    public int getArea()
    {
    	return this.altura * this.base;
    }
}
