package figura;

public class Aritmetica {
	int a,b;
	
	public Aritmetica()
	{
		a=5;
		b=4;
		System.out.println("Ejecutando constructor vacío :D ");
	}
	
	public Aritmetica(int arg1, int arg2)
	{
		
	}

	public int sumar()
	{
		int resultado = a + b;
		return resultado;
	}
}
