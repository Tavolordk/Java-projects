package figura;

public class Persona {
     String nombre;
     String apellido;
     public void desplegarNombre()
   {
	  System.out.println("Nombre: " + nombre);
	  System.out.println("Apellido: " + apellido);
   }
    public static void main()
    {
    	Persona p1 = new Persona();
    	p1.nombre = "Octavio";
    	p1.apellido = "Olea";
    	p1.desplegarNombre();
    }
}
