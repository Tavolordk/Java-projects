	<queryString>
		<![CDATA[SELECT N.ENCABEZADO, N.POLIZA, N.FECHA_EMISION, UPPER(N.TIPO) AS TIPO, FORMAT(N.FECHA_INICIO_VIG,'dd/MM/yyyy') AS FECHA_INICIO_VIG, FORMAT(N.FECHA_FIN_VIG,'dd/MM/yyyy') AS FECHA_FIN_VIG
, UPPER(N.CONTRATANTE) AS CONTRATANTE, N.RFC, UPPER(N.DIRECCION) AS DIRECCION, UPPER(N.ENTIDAD) AS ENTIDAD, N.CP, UPPER(N.AGENTE) AS AGENTE, N.PLAZOSEGURO
, UPPER(N.MONEDA) AS MONEDA, UPPER(N.FORMA_PAGO) AS FORMA_PAGO
, RTRIM(STRING_AGG(UPPER(N.COBERTURA),CHAR(13))) AS COBERTURA
, RTRIM(STRING_AGG(N.SUMA_ASEGURADA,CHAR(13)))  AS SUMA_ASEGURADA
, N.NETA, N.RECARGO, N.GASTOS, N.IVA, TOTAL, N.REGISTRO, N.FECHA_REG
, RTRIM(STRING_AGG(N.TIPO_COB,CHAR(13)) WITHIN GROUP (ORDER BY CVE_COBERTURA ASC))  AS TIPO_COB
, RTRIM(STRING_AGG(N.CVE_COBERTURA,CHAR(13)) WITHIN GROUP (ORDER BY CVE_COBERTURA ASC))  AS CVE_COBERTURA
FROM
(SELECT PRD.encabezado AS ENCABEZADO, PZA.numero_poliza AS POLIZA, FORMAT(PZA.fecha_emision,'dd/MM/yyyy') AS FECHA_EMISION
,CASE poliza_nueva_renovacion
WHEN 0 THEN 'Inicial'
when 1 then 'Renovaciòn'
END AS TIPO 
,fecha_inicio_vigencia AS FECHA_INICIO_VIG,fecha_fin_vigencia AS FECHA_FIN_VIG
,CASE CTE.fisico
WHEN 0 THEN UPPER(CTE.denominacion)
WHEN 1 THEN CTE.nombre_cliente+' '+CTE.apellido_paterno+' '+CTE.apellido_materno
END AS CONTRATANTE 
,CTE.rfc AS RFC, CTE.calle+' '+CTE.numero_exterior AS DIRECCION, EDO.nombre_estado AS ENTIDAD,CTE.codigo_postal AS CP
, AGT.Nombre+' '+AGT.Ape_Paterno+' '+AGT.Ape_Materno AS AGENTE,'1 AÑO' AS PLAZOSEGURO,MON.CT16_Moneda AS MONEDA
,UPPER(SCA.Frecuencia_Pago) AS FORMA_PAGO
, RTRIM(STRING_AGG(CT10.CT10_descripcion,CHAR(13))) AS COBERTURA
,CASE
WHEN CT10.CT10_descripcion LIKE '%ASISTENCIA%' THEN  'AMPARADA'
ELSE RTRIM(ISNULL(CONVERT(VARCHAR,CAST(suma_asegurada_minima AS MONEY),1),0))
END AS SUMA_ASEGURADA
, ISNULL(CONVERT(VARCHAR,CAST(SCA.Prima_Neta AS MONEY),1),0) AS NETA
, ISNULL(CONVERT(VARCHAR,CAST(SCA.Recargo_PagoFraccionado AS MONEY),1),0) AS RECARGO
, ISNULL(CONVERT(VARCHAR,CAST(SCA.DerechoPoliza AS MONEY),1),0) AS GASTOS
, ISNULL(CONVERT(VARCHAR,CAST(SCA.Impuesto_Total AS MONEY),1),0) AS IVA
, ISNULL(CONVERT(VARCHAR,CAST(SCA.Prima_CobroTotal AS MONEY),1),0) AS TOTAL
,PRD.autorizaciones AS REGISTRO
,FORMAT(PRD.fecha_registro,'dd/MM/yyyy') AS FECHA_REG
,CT10.id_tipo_cobertura AS TIPO_COB, CT10.Cve_Cobertura AS CVE_COBERTURA
FROM POLIZAS PZA
LEFT JOIN CLIENTES CTE ON CTE.id_cliente = PZA.id_contratante
LEFT JOIN CT5_ESTADOS EDO ON EDO.id_estado = CTE.id_estado
LEFT JOIN CT13_AGENTES AGT ON AGT.Id_Agente = id_agente_1
LEFT JOIN CT16_MONEDAS MON ON MON.CT16_Cve = cve_moneda_ct16
LEFT JOIN SOLICITUD_CABECERA SCA ON PZA.numero_poliza = SCA.numero_poliza AND SCA.TipoSolicitud = 'AP'
LEFT JOIN COBERTURAS_PRODUCTO  COB ON PZA.cve_producto = COB.id_producto
LEFT JOIN CT10_COBERTURAS CT10 ON Cve_Cobertura = COB.id_cobertura
LEFT JOIN PRODUCTOS PRD ON PRD.Cve_Producto = PZA.cve_producto
WHERE Id_poliza= $P{no_Poliza}
GROUP BY  PRD.encabezado, PZA.numero_poliza,PZA.fecha_emision,poliza_nueva_renovacion,fecha_inicio_vigencia,fecha_fin_vigencia,CTE.fisico,CTE.denominacion,CTE.nombre_cliente
,CTE.apellido_paterno,CTE.apellido_materno,CTE.rfc,CTE.calle,CTE.numero_exterior,EDO.nombre_estado,CTE.codigo_postal,AGT.Nombre,AGT.Ape_Paterno
,AGT.Ape_Materno,MON.CT16_Moneda,SCA.Frecuencia_Pago,SCA.Prima_Neta,SCA.Recargo_PagoFraccionado,SCA.DerechoPoliza,SCA.Impuesto_Total,SCA.Prima_CobroTotal,CT10_descripcion
,suma_asegurada_minima,PRD.autorizaciones,PRD.fecha_registro,PRD.Cve_Producto,CT10.Cve_Cobertura,CT10.id_tipo_cobertura)
AS N
GROUP BY N.ENCABEZADO, N.POLIZA, N.FECHA_EMISION, N.TIPO, N.FECHA_INICIO_VIG, N.FECHA_FIN_VIG, N.CONTRATANTE, N.RFC, N.DIRECCION, N.ENTIDAD, N.CP, N.AGENTE, N.PLAZOSEGURO, N.MONEDA
, N.FORMA_PAGO, N.NETA, N.RECARGO, N.GASTOS, N.IVA, N.TOTAL, N.REGISTRO, N.FECHA_REG]]>
	</queryString>
