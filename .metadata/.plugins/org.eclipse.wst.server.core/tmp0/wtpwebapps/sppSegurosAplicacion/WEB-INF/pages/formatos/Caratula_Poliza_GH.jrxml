	<queryString>
		<![CDATA[SELECT N.TIPOPROD, N.POLIZA, N.FECHA_EMISION, N.INICIO_VIGENCIA, N.FIN_VIGENCIA, N.MONEDA, N.AGENTE, N.FORMA_PAGO, N.CONTRATANTE
, N.RFC, N.DOMICILIO, N.ENTIDAD, N.CP, N.FECHA_CONST
, STRING_AGG(ISNULL(UPPER(n.CVECOB),''), char(13)) WITHIN GROUP (ORDER BY n.CVECOB)  as CVECOB
, RTRIM(STRING_AGG(N.DESCRIPCION_COBERTURA,CHAR(13))) AS DESCRIPCION_COBERTURA
, RTRIM(STRING_AGG(n.PLAZO_COBERTURA,CHAR(13))) AS PLAZO_COBERTURA
, RTRIM(STRING_AGG(n.Suma_Asegurada,CHAR(13))) AS Suma_Asegurada
, N.REINSTALACION
, ISNULL(CONVERT(VARCHAR,CAST(N.NETA AS MONEY),1),0) AS NETA
, N.RECARGO, N.GASTOS_EXP, N.IVA, PRIMA_TOTAL,n.REGISTRO,n.CODIGO,n.VERSION_FEC,n.FECHA_REG
FROM
(SELECT PRD.encabezado AS TIPOPROD
, numero_poliza AS POLIZA, FORMAT(fecha_emision,'dd/MM/yyyy') AS FECHA_EMISION, FORMAT(fecha_inicio_vigencia,'dd/MM/yyyy') AS INICIO_VIGENCIA
, FORMAT(fecha_fin_vigencia,'dd/MM/yyyy') AS FIN_VIGENCIA, UPPER(MON.CT16_Moneda) AS MONEDA
, ISNULL(UPPER(AGT.Nombre+' '+AGT.Ape_Paterno+' '+AGT.Ape_Materno),'') AS AGENTE, 'ANUAL' AS FORMA_PAGO
, CASE fisico
	WHEN 0 THEN UPPER(CTE.denominacion)
	WHEN 1 THEN UPPER(CTE.nombre_cliente+' '+CTE.apellido_paterno+' '+CTE.apellido_materno)
END AS CONTRATANTE
, CTE.rfc AS RFC, UPPER(CTE.calle+' '+CTE.numero_exterior+' '+ ISNULL(MNP.nombre_municipio,'')) AS DOMICILIO
, UPPER(EDO.nombre_estado) AS ENTIDAD, CTE.codigo_postal AS CP
, CASE fisico
	WHEN 0 THEN ''
	WHEN 1 THEN FORMAT(CTE.fecha_constitucion,'dd/MM/yyyy')
END AS FECHA_CONST
, CTC.Cve_Cobertura as CVECOB
, RTRIM(STRING_AGG(ISNULL(CBP.descripcion,'N/D'), char(13))) as DESCRIPCION_COBERTURA
, RTRIM(STRING_AGG(ISNULL(CONVERT(VARCHAR,cbp.plazo_cobertura)+' AÑOS',0), char(13))) as PLAZO_COBERTURA
,CASE PRD.tipo_producto
	WHEN 0 THEN
			(CASE 
			WHEN CTC.CT10_Descripcion NOT LIKE '%ESTRUCTURA%' THEN CONVERT(VARCHAR, ISNULL(CAST(CBP.porcentaje_sa_indemnizacion AS decimal(10,2)),0)) + '% DEL VALOR DE ESCRITURACIÓN'
			ELSE ISNULL(CONVERT(VARCHAR,CAST(POL.suma_asegurada AS MONEY),1),0) END)
	ELSE ISNULL(CONVERT(VARCHAR,CAST(POL.suma_asegurada AS MONEY),1),0)
END AS Suma_Asegurada
,CASE PRD.tipo_producto
WHEN 0 THEN 'Amparada'
ELSE ''
END AS REINSTALACION
, CAST(prima_neta_total AS numeric) - (CAST(recargo_pago_fraccionado AS numeric) + CAST(PRD.derecho_poliza AS numeric) + CAST(impuesto_total AS numeric)) AS NETA
, ISNULL(CONVERT(VARCHAR,CAST(recargo_pago_fraccionado AS MONEY),1),0) AS RECARGO
, ISNULL(CONVERT(VARCHAR,CAST(PRD.derecho_poliza AS MONEY),1),0) AS GASTOS_EXP,ISNULL(CONVERT(VARCHAR,CAST(impuesto_total AS MONEY),1),0) AS IVA
, ISNULL(CONVERT(VARCHAR,CAST(prima_neta_total AS MONEY),1),0) AS PRIMA_TOTAL
,PRD.autorizaciones AS REGISTRO
,FORMAT(PRD.fecha_registro,'dd/MM/yyyy') AS FECHA_REG
, CASE PRD.tipo_producto
WHEN 0 THEN 'SPP-FR-PD-15'
WHEN 1 THEN 'SPP-FR-PD-21'
END AS CODIGO
, PRD.version_fecha AS VERSION_FEC 
  FROM POLIZAS POL
LEFT JOIN PRODUCTOS PRD ON PRD.Cve_Producto = POL.cve_producto
LEFT JOIN CT16_MONEDAS MON ON MON.CT16_Cve = POL.cve_moneda_ct16
LEFT JOIN CT13_AGENTES AGT ON AGT.Id_Agente = POL.id_agente_1
LEFT JOIN CLIENTES CTE ON CTE.id_cliente = POL.id_contratante
LEFT JOIN CT6_MUNICIPIOS MNP ON MNP.id_municipio = CTE.id_municipio
LEFT JOIN CT5_ESTADOS EDO ON EDO.id_estado = CTE.id_estado
LEFT JOIN CUVS CUV ON CUV.id_cuv = POL.id_ov
LEFT JOIN COBERTURAS_PRODUCTO CBP ON CBP.id_producto = POL.cve_producto
LEFT JOIN CT10_COBERTURAS CTC ON CTC.Cve_Cobertura = CBP.id_cobertura
WHERE id_poliza = $P{no_Poliza} AND endoso = 0
GROUP BY PRD.tipo_producto, numero_poliza, fecha_emision, fecha_inicio_vigencia, fecha_fin_vigencia, MON.CT16_Moneda, AGT.Nombre, AGT.Ape_Paterno
, AGT.Ape_Materno, fisico, CTE.denominacion, CTE.nombre_cliente, CTE.apellido_paterno, CTE.apellido_materno, CTE.rfc, CTE.calle 
, CTE.numero_exterior, MNP.nombre_municipio, EDO.nombre_estado, CTE.codigo_postal, CTE.fecha_constitucion, CTC.Cve_Cobertura
, CUV.id_oferta_vivienda, CBP.suma_asegurada_minima, prima_neta_total, recargo_pago_fraccionado, PRD.derecho_poliza, impuesto_total
, CTC.CT10_Descripcion, POL.suma_asegurada, CBP.porcentaje_sa_indemnizacion,PRD.encabezado,PRD.autorizaciones,PRD.fecha_registro,PRD.version_fecha)
AS N
GROUP BY 
N.TIPOPROD, N.POLIZA, N.FECHA_EMISION, N.INICIO_VIGENCIA, N.FIN_VIGENCIA, N.MONEDA, N.AGENTE, N.FORMA_PAGO, N.CONTRATANTE
, N.RFC, N.DOMICILIO, N.ENTIDAD, N.CP, N.FECHA_CONST, N.REINSTALACION, N.NETA, N.RECARGO, N.GASTOS_EXP, N.IVA, N.PRIMA_TOTAL
,n.REGISTRO,n.CODIGO,n.VERSION_FEC,n.FECHA_REG]]>
