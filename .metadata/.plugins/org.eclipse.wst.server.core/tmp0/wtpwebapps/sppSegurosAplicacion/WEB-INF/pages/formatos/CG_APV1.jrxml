	<queryString>
		<![CDATA[SELECT PRD.encabezado AS ENCABEZADO, numero_poliza, '' AS no_Certificado
		,CASE CTE.fisico
		WHEN 0 THEN UPPER(CTE.denominacion)
		WHEN 1 THEN UPPER(CTE.nombre_cliente+' '+CTE.apellido_paterno+' '+CTE.apellido_materno)
		END AS nombre_cliente
		FROM POLIZAS
		LEFT JOIN CLIENTES CTE ON CTE.id_cliente = id_contratante
		LEFT JOIN PRODUCTOS PRD ON PRD.Cve_Producto = POLIZAS.cve_producto
		WHERE id_poliza = $P{no_Poliza}]]>
	</queryString>
