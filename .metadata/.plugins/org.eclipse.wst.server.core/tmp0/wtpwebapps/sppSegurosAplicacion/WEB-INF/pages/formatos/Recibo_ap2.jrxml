	<queryString>
		<![CDATA[SELECT
CASE PLZ.cve_ramo
WHEN 5 THEN '1 DE 1'
WHEN 1 THEN CONVERT(VARCHAR,RECIBO) + ' DE '+(CASE sca.Frecuencia_Pago
WHEN 'ANUAL' THEN '1'
WHEN 'SEMESTRAL' THEN '2'
WHEN 'TRIMESTRAL' THEN '4'
WHEN 'MENSUAL' THEN '12'
END)
END AS SERIE
--,CASE PLZ.cve_ramo
--WHEN 5 THEN 'Garantía Hogar'
--WHEN 1 THEN 'AP-SPP'
--END AS RAMO
,PRD.encabezado AS RAMO
,'CDMX' AS LUGAR,RECIBO,FORMAT(INICIO_VIGENCIA,'dd/MM/yyyy') AS INICIO_VIGENCIA,FORMAT(FIN_VIGENCIA,'dd/MM/yyyy') AS FIN_VIGENCIA
,FORMAT(RECIBOS.FECHA_EMISION,'dd/MM/yyyy') AS FECHA
,CONVERT(VARCHAR,plz.numero_poliza) + ' - ' + CONVERT(VARCHAR,RECIBOS.ENDOSO) AS numero_poliza,'' AS ID_CERTIFICADO
,CASE cte.fisico
WHEN 0 THEN UPPER(cte.denominacion)
WHEN 1 THEN UPPER(cte.nombre_cliente+' '+cte.apellido_paterno+' '+cte.apellido_materno)
END AS CONTRATANTE
,ISNULL(cte.rfc,'') AS RFC
,ISNULL(UPPER(cte.calle+' '+cte.numero_exterior+', '+MNP.nombre_municipio+', '+EDO.nombre_estado),'') as DIRECCION
--,AAP.nombre_asegurado+' '+AAP.apellido_paterno_asegurado+' '+AAP.apellido_materno_asegurado AS NOMBRE_ASE
,ISNULL(crt.Asegurado,'') AS NOMBRE_ASE
,ISNULL(UPPER(crt.calle+', '+crt.colonia+', '+MNP2.nombre_municipio+', '+EDO2.nombre_estado),'') AS DIRECCION_ASE
,ISNULL(CONVERT(VARCHAR,CAST(RECIBOS.PRIMA_NETA AS MONEY),1),0) AS NETA,ISNULL(CONVERT(VARCHAR,CAST(RECARGO_PAGO_FRACC AS MONEY),1),0) AS RECARGO
,ISNULL(CONVERT(VARCHAR,CAST(RECIBOS.DERECHO_POLIZA AS MONEY),1),0) AS GASTOS,ISNULL(CONVERT(VARCHAR,CAST(RECIBOS.IMPUESTO_TOTAL AS MONEY),1),0) AS IVA
,ISNULL(CONVERT(VARCHAR,CAST(PRIMA_TOTAL AS MONEY),1),0) AS PRIMA_TOTAL
,id_agente_1 AS CLAVEAGENTE , ISNULL(UPPER(agt.Nombre+' '+agt.Ape_Paterno+' '+ agt.Ape_Materno),'') as AGENTE
,ISNULL(CT16_MONEDA,'') AS MONEDA
,CASE PRD.tipo_producto
	WHEN 2 THEN UPPER(sca.Frecuencia_Pago)
	ELSE crt.forma_pago
END AS FORMA_PAGO
, UPPER(FPA.CT18_Descripcion) AS INSTRUMENTO_COBRO
, ISNULL(REF_SANTANDER, '') AS REFERENCIA
, 'SANTANDER' AS BANCO
FROM RECIBOS
LEFT JOIN POLIZAS plz ON plz.id_poliza = POLIZA
LEFT JOIN CLIENTES cte ON cte.id_cliente = plz.Id_Contratante
--LEFT JOIN CLIENTES cts ON cts.id_cliente = plz.id_asegurado
LEFT JOIN CERTIFICADO crt ON crt.id_certificado = RECIBOS.ID_CERTIFICADO
LEFT JOIN HABITABILIDAD hab ON hab.numero_credito =  crt.numero_credito
LEFT JOIN CT13_AGENTES agt ON agt.Id_Agente = plz.id_agente_1
LEFT JOIN CT16_MONEDAS ON CT16_Cve = ID_MONEDA
LEFT JOIN SOLICITUD_CABECERA sca ON plz.numero_poliza = sca.numero_poliza AND SCA.TipoSolicitud = 'AP'
LEFT JOIN PRODUCTOS PRD ON PRD.Cve_Producto = plz.cve_producto
LEFT JOIN CT6_MUNICIPIOS MNP ON MNP.id_municipio = CTE.id_municipio
LEFT JOIN CT5_ESTADOS EDO ON EDO.id_estado = CTE.id_estado
LEFT JOIN CT6_MUNICIPIOS MNP2 ON MNP2.id_municipio = crt.id_municipio
LEFT JOIN CT5_ESTADOS EDO2 ON EDO2.id_estado = crt.id_estado
--INNER JOIN ASEGURADOS_AP AAP ON AAP.id_poliza = plz.id_poliza
LEFT JOIN CT18_FORMASPAGO FPA ON FPA.CT18_Cve = cte.metodo_usual_pago
WHERE RECIBOS.ID_RECIBO=$P{no_Poliza}]]>
