	<queryString>
		<![CDATA[SELECT N.ENCABEZADO, N.POLIZA, N.FECHA_EMISION, N.CERTIFICADO, UPPER(N.TIPO) AS TIPO, FORMAT(N.FECHA_INICIO_VIG,'dd/MM/yyyy') AS FECHA_INICIO_VIG
, FORMAT(N.FECHA_FIN_VIG,'dd/MM/yyyy') AS FECHA_FIN_VIG, N.CONTRATANTE, N.RFC, N.DIRECCION, UPPER(N.ENTIDAD) AS ENTIDAD, N.CP
, FORMAT(N.FECHA_NACIMIENTO,'dd/MM/yyyy') AS FECHA_NACIMIENTO, N.AGENTE, N.PLAZOSEGURO, UPPER(N.MONEDA) AS MONEDA
, N.FORMA_PAGO, UPPER(N.ASEGURADO) AS ASEGURADO, FORMAT(N.FECHA_NACIMIENTO_AP,'dd/MM/yyyy') AS FECHA_NACIMIENTO_AP, N.EDAD, N.SEXO
, RTRIM(STRING_AGG(UPPER(N.COBERTURA),CHAR(13))) AS COBERTURA
, RTRIM(STRING_AGG(N.SUMA_ASEGURADA,CHAR(13))) AS SUMA_ASEGURADA
, N.BENEFICIARIO1, N.BENEFICIARIO2, N.BENEFICIARIO3, N.BENEFICIARIO4, N.BENEFICIARIO5
, N.PARENTEZCO1, N.PARENTEZCO2, N.PARENTEZCO3, N.PARENTEZCO4, N.PARENTEZCO5
, N.PORCENTAJE1, N.PORCENTAJE2, N.PORCENTAJE3, N.PORCENTAJE4, N.PORCENTAJE5
--, N.NETA/CONTADOR AS NETA , N.RECARGO, N.GASTOS, N.IVA, N.TOTAL
,CONVERT(VARCHAR,CAST(N.NETA/CONTADOR AS MONEY),1) AS NETA
,CONVERT(VARCHAR,CAST(N.RECARGO/CONTADOR AS MONEY),1) AS RECARGO
,CONVERT(VARCHAR,CAST(N.GASTOS/CONTADOR AS MONEY),1) AS GASTOS
,CONVERT(VARCHAR,CAST(N.IVA/CONTADOR AS MONEY),1) AS IVA
,CONVERT(VARCHAR,CAST(N.TOTAL/CONTADOR AS MONEY),1) AS TOTAL
, N.REGISTRO, N.FECHA_REG
, RTRIM(STRING_AGG(N.TIPO_COB,CHAR(13)) WITHIN GROUP (ORDER BY CVE_COBERTURA ASC))  AS TIPO_COB
, RTRIM(STRING_AGG(N.CVE_COBERTURA,CHAR(13)) WITHIN GROUP (ORDER BY CVE_COBERTURA ASC))  AS CVE_COBERTURA
FROM
(SELECT PRD.encabezado AS ENCABEZADO, PZA.numero_poliza AS POLIZA, FORMAT(PZA.fecha_emision,'dd/MM/yyyy') AS FECHA_EMISION
,AAP.num_Asegurado AS CERTIFICADO 
,(select count(*) from ASEGURADOS_AP where id_poliza = $P{no_Poliza}) AS CONTADOR
,CASE poliza_nueva_renovacion
WHEN 0 THEN 'Inicial'
when 1 then 'Renovaciòn'
END AS TIPO
,fecha_inicio_vigencia AS FECHA_INICIO_VIG,fecha_fin_vigencia AS FECHA_FIN_VIG
,CASE CTE.fisico
WHEN 0 THEN UPPER(CTE.denominacion)
WHEN 1 THEN CTE.nombre_cliente+' '+CTE.apellido_paterno+' '+CTE.apellido_materno
END AS CONTRATANTE 
,CTE.rfc AS RFC, CTE.calle+' '+CTE.numero_exterior AS DIRECCION, EDO.nombre_estado AS ENTIDAD,CTE.codigo_postal AS CP
,CASE CTE.fisico
WHEN 0 THEN CTE.fecha_constitucion
WHEN 1 THEN CTE.fecha_nacimiento
END AS FECHA_NACIMIENTO
, AGT.Nombre+' '+AGT.Ape_Paterno+' '+AGT.Ape_Materno AS AGENTE,'1 AÑO' AS PLAZOSEGURO, MON.CT16_Moneda AS MONEDA
,UPPER(SCA.Frecuencia_Pago) AS FORMA_PAGO,AAP.nombre_asegurado+' '+AAP.apellido_paterno_asegurado+' '+AAP.apellido_materno_asegurado AS ASEGURADO
,AAP.fecha_nacimiento AS FECHA_NACIMIENTO_AP, (cast(datediff(dd,AAP.fecha_nacimiento,GETDATE()) / 365.25 as int)) AS EDAD
,CASE AAP.sexo 
WHEN 'F' THEN 'FEMENINO'
WHEN 'M' THEN 'MASCULINO'
ELSE ''
END AS SEXO
, RTRIM(STRING_AGG(CT.CT10_Descripcion,CHAR(13))) AS COBERTURA
,CASE
WHEN CT.CT10_descripcion LIKE '%ASISTENCIA%' THEN  'AMPARADA'
ELSE RTRIM(ISNULL(CONVERT(VARCHAR,CAST(COB.suma_asegurada_minima AS MONEY),1),0))
END AS SUMA_ASEGURADA
,UPPER(AAP.nombre_beneficiario_1) AS BENEFICIARIO1
,UPPER(AAP.nombre_beneficiario_2) AS BENEFICIARIO2
,UPPER(AAP.nombre_beneficiario_3) AS BENEFICIARIO3
,UPPER(AAP.nombre_beneficiario_4) AS BENEFICIARIO4
,UPPER(AAP.nombre_beneficiario_5) AS BENEFICIARIO5
,CASE WHEN AAP.parentesco_1 IS NULL THEN '' WHEN AAP.porcentaje_1 = 0 THEN '' ELSE PAR.descripcion END AS PARENTEZCO1
,CASE WHEN AAP.parentesco_2 IS NULL THEN '' WHEN AAP.porcentaje_2 = 0 THEN '' ELSE PAR2.descripcion END AS PARENTEZCO2
,CASE WHEN AAP.parentesco_3 IS NULL THEN '' WHEN AAP.porcentaje_3 = 0 THEN '' ELSE PAR3.descripcion END AS PARENTEZCO3
,CASE WHEN AAP.parentesco_4 IS NULL THEN '' WHEN AAP.porcentaje_4 = 0 THEN '' ELSE PAR4.descripcion END AS PARENTEZCO4
,CASE WHEN AAP.parentesco_5 IS NULL THEN '' WHEN AAP.porcentaje_5 = 0 THEN '' ELSE PAR5.descripcion END AS PARENTEZCO5
,CASE WHEN AAP.porcentaje_1 = 0 THEN NULL ELSE AAP.porcentaje_1 END AS PORCENTAJE1
,CASE WHEN AAP.porcentaje_2 = 0 THEN NULL ELSE AAP.porcentaje_2 END AS PORCENTAJE2
,CASE WHEN AAP.porcentaje_3 = 0 THEN NULL ELSE AAP.porcentaje_3 END AS PORCENTAJE3
,CASE WHEN AAP.porcentaje_4 = 0 THEN NULL ELSE AAP.porcentaje_4 END AS PORCENTAJE4
,CASE WHEN AAP.porcentaje_5 = 0 THEN NULL ELSE AAP.porcentaje_5 END AS PORCENTAJE5
,ISNULL(SCA.Prima_Neta,0) AS NETA
,ISNULL(SCA.Recargo_PagoFraccionado,0) AS RECARGO
,ISNULL(SCA.derechoPoliza,0) AS GASTOS
,ISNULL(SCA.Impuesto_Total,0) AS IVA
,ISNULL(SCA.Prima_CobroTotal,0) AS TOTAL
,PRD.autorizaciones AS REGISTRO
,FORMAT(PRD.fecha_registro,'dd/MM/yyyy') AS FECHA_REG
,CT.id_tipo_cobertura AS TIPO_COB, CT.Cve_Cobertura AS CVE_COBERTURA
FROM POLIZAS PZA
INNER JOIN ASEGURADOS_AP AAP ON AAP.id_poliza = PZA.id_poliza
LEFT JOIN CLIENTES CTE ON CTE.id_cliente = PZA.id_contratante
LEFT JOIN CT5_ESTADOS EDO ON EDO.id_estado = CTE.id_estado
LEFT JOIN CT13_AGENTES AGT ON AGT.Id_Agente = id_agente_1
LEFT JOIN CT16_MONEDAS MON ON MON.CT16_Cve = cve_moneda_ct16
LEFT JOIN COBERTURAS_PRODUCTO  COB ON PZA.cve_producto = COB.id_producto
LEFT JOIN SOLICITUD_CABECERA SCA ON PZA.numero_poliza = SCA.numero_poliza AND SCA.TipoSolicitud = 'AP'
LEFT JOIN CT10_COBERTURAS CT ON CT.Cve_Cobertura = COB.id_cobertura
LEFT JOIN CT26_PARENTESCO PAR ON PAR.id_parentesco = AAP.parentesco_1
LEFT JOIN CT26_PARENTESCO PAR2 ON PAR2.id_parentesco = AAP.parentesco_2
LEFT JOIN CT26_PARENTESCO PAR3 ON PAR3.id_parentesco = AAP.parentesco_3
LEFT JOIN CT26_PARENTESCO PAR4 ON PAR4.id_parentesco = AAP.parentesco_4
LEFT JOIN CT26_PARENTESCO PAR5 ON PAR5.id_parentesco = AAP.parentesco_5
LEFT JOIN PRODUCTOS PRD ON PRD.Cve_Producto = PZA.cve_producto
WHERE PZA.Id_poliza= $P{no_Poliza}
GROUP BY PRD.encabezado,PZA.numero_poliza,PZA.fecha_emision,AAP.num_Asegurado,poliza_nueva_renovacion,fecha_inicio_vigencia,fecha_fin_vigencia,CTE.nombre_cliente
,CTE.apellido_paterno,CTE.apellido_materno,CTE.rfc,CTE.calle,CTE.numero_exterior,EDO.nombre_estado,CTE.codigo_postal,CTE.fecha_constitucion
,CTE.fecha_nacimiento,AGT.Nombre,AGT.Ape_Paterno,AGT.Ape_Materno,MON.CT16_Moneda,prima_neta_total,recargo_pago_fraccionado
,PZA.derecho_poliza,PZA.impuesto_total,CTE.fisico,CTE.denominacion,fecha_constitucion,SCA.Frecuencia_Pago,AAP.nombre_asegurado,AAP.apellido_paterno_asegurado
,AAP.apellido_materno_asegurado,AAP.fecha_nacimiento,AAP.sexo
,AAP.nombre_beneficiario_1,AAP.nombre_beneficiario_2,AAP.nombre_beneficiario_3,AAP.nombre_beneficiario_4,AAP.nombre_beneficiario_5
,AAP.parentesco_1,AAP.parentesco_2,AAP.parentesco_3,AAP.parentesco_4,AAP.parentesco_5
,AAP.porcentaje_1,AAP.porcentaje_2,AAP.porcentaje_3,AAP.porcentaje_4,AAP.porcentaje_5
,SCA.Prima_Neta,SCA.Recargo_PagoFraccionado,SCA.derechoPoliza,SCA.Impuesto_Total
,PAR.descripcion,PAR2.descripcion,PAR3.descripcion,PAR4.descripcion,PAR5.descripcion,SCA.Prima_CobroTotal,PZA.Id_poliza,CT.CT10_Descripcion
,COB.suma_asegurada_minima,PRD.autorizaciones,PRD.fecha_registro,CT.Cve_Cobertura,CT.id_tipo_cobertura)
AS N
GROUP BY 
N.ENCABEZADO, N.POLIZA, N.FECHA_EMISION, N.CERTIFICADO, N.TIPO, N.FECHA_INICIO_VIG, N.FECHA_FIN_VIG, N.CONTRATANTE, N.RFC, N.DIRECCION, N.ENTIDAD, N.CP, N.FECHA_NACIMIENTO
, N.AGENTE, N.PLAZOSEGURO, N.MONEDA, N.FORMA_PAGO, N.ASEGURADO, N.FECHA_NACIMIENTO_AP, N.EDAD, N.SEXO
, N.BENEFICIARIO1, N.BENEFICIARIO2, N.BENEFICIARIO3, N.BENEFICIARIO4, N.BENEFICIARIO5
, N.PARENTEZCO1, N.PARENTEZCO2, N.PARENTEZCO3, N.PARENTEZCO4, N.PARENTEZCO5
, N.PORCENTAJE1, N.PORCENTAJE2, N.PORCENTAJE3, N.PORCENTAJE4, N.PORCENTAJE5
, N.NETA , N.RECARGO, N.GASTOS, N.IVA, N.TOTAL, N.CONTADOR, N.REGISTRO, N.FECHA_REG]]>
	</queryString>
