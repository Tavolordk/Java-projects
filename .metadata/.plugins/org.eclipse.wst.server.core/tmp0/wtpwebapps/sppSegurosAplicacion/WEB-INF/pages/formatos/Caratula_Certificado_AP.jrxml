			<staticText>
				<reportElement style="styleLabel" positionType="Float" mode="Opaque" x="1" y="678" width="571" height="35" forecolor="#FFFFFF" backcolor="#FFFFFF" uuid="65a6afe0-760b-4c0b-afba-f0dabcf3678c"/>
