	<queryString>
		<![CDATA[SELECT 
n.TIPOPROD,n.POLIZA,n.FECHA_EMISION,n.CERTIFICADO,n.INICIO_VIGENCIA,n.FIN_VIGENCIA,n.MONEDA,n.AGENTE,n.FORMA_PAGO,n.ASEGURADO_BENEFICIARIO,n.RFC,
n.DOMICILIO,n.CP,n.ENTIDAD_ASEG,n.NUMSEGURIDADSOC,n.cuv,n.DOMICILIO_BIENASEG,n.codigo_postal,n.ENTIDAD,
STRING_AGG(ISNULL(UPPER(n.CVECOB),''), char(13)) WITHIN GROUP (ORDER BY n.CVECOB ASC)  as CVECOB,
RTRIM(STRING_AGG(n.DESCRIPCION_COBERTURA,CHAR(13))) AS DESCRIPCION_COBERTURA,
RTRIM(STRING_AGG(n.PLAZO_COBERTURA,CHAR(13))) AS PLAZO_COBERTURA,
RTRIM(STRING_AGG(n.Suma_Asegurada,CHAR(13))) AS Suma_Asegurada,
RTRIM(STRING_AGG(n.deducible,CHAR(13))) AS deducible,n.REINSTALACION, n.ENDOSOS,
n.NETA,n.RECARGO,n.GASTOS_EXP,n.IVA,n.PRIMA_TOTAL,n.REGISTRO,n.CODIGO,n.VERSION_FEC,n.FECHA_REG
FROM
(SELECT PRD.encabezado AS TIPOPROD
,PZS.numero_poliza AS POLIZA,FORMAT(PZS.fecha_emision,'dd/MM/yyyy') AS FECHA_EMISION,CERTIFICADO.numero_credito AS CERTIFICADO,FORMAT(PZS.fecha_inicio_vigencia,'dd/MM/yyyy') AS INICIO_VIGENCIA
,FORMAT(PZS.fecha_fin_vigencia,'dd/MM/yyyy') AS FIN_VIGENCIA, ISNULL(UPPER(MND.CT16_Moneda),'') AS MONEDA
,ISNULL(UPPER(AGT.Nombre+' '+AGT.Ape_Paterno+' '+AGT.Ape_Materno),'') AS AGENTE,ISNULL(forma_pago,'ANUAL') AS FORMA_PAGO
,ISNULL(HAB.nss_acreditado,'') AS NUMSEGURIDADSOC
,CERTIFICADO.cuv,UPPER(CERTIFICADO.calle+' '+CERTIFICADO.colonia+' '+ISNULL(MNP.nombre_municipio,'')) AS DOMICILIO_BIENASEG
,CERTIFICADO.codigo_postal,UPPER(EDO.nombre_estado) AS ENTIDAD
,UPPER(CERTIFICADO.Asegurado) AS ASEGURADO_BENEFICIARIO
,CASE PRD.tipo_producto
WHEN 0 THEN CTE.rfc
ELSE ''
END AS RFC
,CASE PRD.tipo_producto
WHEN 0 THEN UPPER(CTE.calle+' '+CTE.numero_exterior+' '+CTE.numero_interior+' '+ISNULL(MN2.nombre_municipio,''))
ELSE UPPER(CERTIFICADO.calle+' '+CERTIFICADO.colonia+' '+ISNULL(MNP.nombre_municipio,''))
END AS DOMICILIO
,CASE PRD.tipo_producto
WHEN 0 THEN UPPER(EDOC.nombre_estado)
ELSE UPPER(EDO.nombre_estado)
END AS ENTIDAD_ASEG
,CASE PRD.tipo_producto
WHEN 0 THEN CTE.codigo_postal
ELSE CERTIFICADO.codigo_postal
END AS CP 
,STRING_AGG(ISNULL(UPPER(CTC.Cve_Cobertura),''), char(13)) as CVECOB
,RTRIM(STRING_AGG(ISNULL(UPPER(CTC.CT10_Descripcion),''), char(13))) as DESCRIPCION_COBERTURA,
RTRIM(STRING_AGG(ISNULL(CONVERT(VARCHAR,cbp.plazo_cobertura)+' AÑOS',0), char(13))) as PLAZO_COBERTURA
,CASE PRD.tipo_producto
	WHEN 0 THEN
			(CASE 
			WHEN CTC.CT10_Descripcion NOT LIKE '%ESTRUCTURA%' THEN '3% DEL VALOR DE ESCRITURACIÓN'
			ELSE ISNULL(CONVERT(VARCHAR,CAST(CERTIFICADO.suma_asegurada AS MONEY),1),0) END)
	ELSE ISNULL(CONVERT(VARCHAR,CAST(CERTIFICADO.suma_asegurada AS MONEY),1),0)
END AS Suma_Asegurada
--ISNULL(CONVERT(VARCHAR,CAST(CERTIFICADO.suma_asegurada AS MONEY),1),0) AS Suma_Asegurada
,ISNULL(CONVERT(VARCHAR,CAST(cbp.deducible AS MONEY),1),0) AS deducible
,CASE PRD.tipo_producto
WHEN 1 THEN 'Amparada'
ELSE ''
END AS REINSTALACION
,CASE PRD.tipo_producto
WHEN 1 THEN 'Beneficiario Preferente'
ELSE ''
END AS ENDOSOS
,ISNULL(CONVERT(VARCHAR,CAST(RCB.PRIMA_NETA AS MONEY),1),0) AS NETA,ISNULL(CONVERT(VARCHAR,CAST(RCB.RECARGO_PAGO_FRACC AS MONEY),1),0) AS RECARGO
,ISNULL(CONVERT(VARCHAR,CAST(RCB.DERECHO_POLIZA AS MONEY),1),0) AS GASTOS_EXP,ISNULL(CONVERT(VARCHAR,CAST(RCB.IMPUESTO_TOTAL AS MONEY),1),0) AS IVA
,ISNULL(CONVERT(VARCHAR,CAST(RCB.PRIMA_TOTAL AS MONEY),1),0) AS PRIMA_TOTAL
,PRD.autorizaciones AS REGISTRO
,FORMAT(PRD.fecha_registro,'dd/MM/yyyy') AS FECHA_REG
, CASE PRD.tipo_producto
WHEN 0 THEN 'SPP-FR-PD-14'
WHEN 1 THEN 'SPP-FR-PD-20'
END AS CODIGO
, PRD.version_fecha AS VERSION_FEC
FROM CERTIFICADO
INNER JOIN POLIZAS PZS ON PZS.id_poliza = CERTIFICADO.poliza
LEFT JOIN CT16_MONEDAS MND ON MND.CT16_Cve = cve_moneda_ct16
LEFT JOIN CT13_AGENTES AGT ON AGT.Id_Agente = PZS.id_agente_1
LEFT JOIN HABITABILIDAD HAB ON HAB.numero_credito = CERTIFICADO.numero_credito
LEFT JOIN CT6_MUNICIPIOS MNP ON MNP.id_municipio = CERTIFICADO.id_municipio
LEFT JOIN CT5_ESTADOS EDO ON EDO.id_estado = CERTIFICADO.id_estado
LEFT JOIN COBERTURAS_PRODUCTO CBP ON CBP.id_producto = PZS.cve_producto
LEFT JOIN RECIBOS RCB ON CERTIFICADO.id_certificado = RCB.ID_CERTIFICADO AND RCB.PRIMA_NETA NOT LIKE '%-%' AND RCB.FECHA_VIGENTE is not null
LEFT JOIN PRODUCTOS PRD ON PRD.Cve_Producto = PZS.cve_producto
LEFT JOIN CLIENTES CTE ON CTE.id_cliente = PZS.id_contratante
LEFT JOIN CT6_MUNICIPIOS MN2 ON MN2.id_municipio = CTE.id_municipio
LEFT JOIN CT5_ESTADOS EDOC ON EDOC.id_estado = CTE.id_estado
LEFT JOIN CT10_COBERTURAS CTC ON CTC.Cve_Cobertura = CBP.id_cobertura
WHERE CERTIFICADO.id_certificado =$P{no_Poliza}
GROUP BY PZS.numero_poliza,CERTIFICADO.numero_credito,PZS.fecha_inicio_vigencia,PZS.fecha_fin_vigencia,MND.CT16_Moneda,AGT.Nombre
,AGT.Ape_Paterno,AGT.Ape_Materno,PZS.cve_producto,CERTIFICADO.Asegurado,PZS.anexo_1,HAB.nombre_completo_acreditado,HAB.calle,HAB.colonia,HAB.municipio_vivienda
,HAB.cp,HAB.nss_acreditado,CERTIFICADO.cuv,CERTIFICADO.calle,CERTIFICADO.colonia,MNP.nombre_municipio,CERTIFICADO.codigo_postal
,EDO.nombre_estado,RCB.PRIMA_NETA,RCB.RECARGO_PAGO_FRACC,RCB.DERECHO_POLIZA,RCB.IMPUESTO_TOTAL,RCB.PRIMA_TOTAL,forma_pago,CBP.descripcion,CERTIFICADO.suma_asegurada,cbp.deducible
,PRD.tipo_producto,CTE.nombre_cliente,CTE.apellido_paterno,CTE.apellido_materno,CTE.fisico,CTE.denominacion,CTE.rfc,CTE.calle,CTE.numero_exterior,CTE.numero_interior,MN2.nombre_municipio
,EDOC.nombre_estado,CTE.codigo_postal,CTC.Cve_Cobertura,PRD.tipo_producto,CTC.CT10_Descripcion,PZS.fecha_emision,PRD.tipo_producto
,PRD.autorizaciones,PRD.fecha_registro, PRD.version_fecha,PRD.encabezado) 
as n
GROUP BY 
n.TIPOPROD,n.POLIZA,n.FECHA_EMISION,n.CERTIFICADO,n.INICIO_VIGENCIA,n.FIN_VIGENCIA,
n.MONEDA,n.AGENTE,n.FORMA_PAGO,n.RFC,n.ASEGURADO_BENEFICIARIO,
n.DOMICILIO,n.CP,n.ENTIDAD_ASEG,n.NUMSEGURIDADSOC,n.cuv,n.DOMICILIO_BIENASEG,
n.codigo_postal,n.ENTIDAD,
n.NETA,n.RECARGO,n.GASTOS_EXP,n.IVA,n.PRIMA_TOTAL,n.REINSTALACION,n.ENDOSOS,n.REGISTRO,n.CODIGO,n.VERSION_FEC,n.FECHA_REG]]>
	</queryString>
