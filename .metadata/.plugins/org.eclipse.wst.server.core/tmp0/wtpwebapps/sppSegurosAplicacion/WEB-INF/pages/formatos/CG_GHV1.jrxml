					<textElement textAlignment="Justified" markup="html">
						<paragraph rightIndent="8"/>
					</textElement>
					<text><![CDATA[<b>Por cualquier tipo de contaminación. </b>]]></text>
				</staticText>
				<staticText>
					<reportElement x="1" y="118" width="22" height="39" uuid="4e9fb7a4-497c-4270-9c86-77b7486f664b"/>
