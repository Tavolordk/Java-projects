	<queryString>
		<![CDATA[select Numero_Poliza,numero_certificados ,
CASE cte.fisico
WHEN 0 THEN ISNULL(cte.denominacion,'')
WHEN 1 THEN ISNULL(cte.nombre_cliente + ' ' + cte.apellido_paterno + ' ' + cte.apellido_materno,'')
END  AS Expedida
from POLIZAS
LEFT JOIN CLIENTES cte ON id_contratante = cte.id_cliente
WHERE id_poliza=$P{no_Poliza}]]>
	</queryString>
